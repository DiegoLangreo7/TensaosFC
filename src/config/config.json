{
  "site": {
    "title": "TensaosFC",
    "base_url": "https://astroplate.netlify.app",
    "base_path": "/",
    "trailing_slash": false,
    "favicon": "/images/escudo.png",
    "logo": "/images/logo.png",
    "logo_darkmode": "/images/logo-dark.png",
    "logo_width": "150",
    "logo_height": "30",
    "logo_text": "Astroplate"
  },

  "settings": {
    "sticky_header": true,
    "theme_switcher": true,
    "default_theme": "system",
    "pagination": 2,
    "summary_length": 200,
    "blog_folder": "blog"
  },

  "params": {
    "contact_form_action": "#",
    "copyright": "Developed by [DiegoLangreo7](https://github.com/DiegoLangreo7)"
  },

  "google_tag_manager": {
    "enable": false,
    "gtm_id": "GTM-XXXXXX"
  },

  "disqus": {
    "enable": true,
    "shortname": "themefisher-template",
    "settings": {}
  },

  "metadata": {
    "meta_author": "zeon.studio",
    "meta_image": "/images/og-image.png",
    "meta_description": "astro and tailwind boilerplate"
  }
}
